{
  "name": "warcraft-guild",
  "version": "0.0.1",
  "description": "Gather guild member data from a variety of sources and expose the data in a public URL that can be consumed.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "graphql:warcraftlogs": "graphql-codegen --config codegen.yml",
    "build": "run-s graphql:warcraftlogs tsc",
    "start:server": "node dist/index.js",
    "start": "run-s build start:server"
  },
  "dependencies": {
    "express": "^4.15.2",
    "got": "^11.3.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.10",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/papaparse": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "typescript": "^4.2.3"
  }
}
